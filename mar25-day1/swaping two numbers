//c
#include <stdio.h>

int main() {
    // Write C code here
    int a,b;
    scanf("%d %d",&a,&b);
    int temp = 0;
    temp = a;
    a = b;
    b = temp;
    printf("after swaping %d %d",a,b);
    //printf("%d ",b);
    return 0;
}

#python

a,b = map(int,input("enter the numbers:").split())
#a,b = b,a
temp =0
temp = a
a = b
b = temp
print("after swaping :",a ,b)

output

enter the numbers:1 4
after swaping : 4 1

=== Code Execution Successful ===


