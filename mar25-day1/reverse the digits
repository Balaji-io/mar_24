#include <stdio.h>

int main() {
    // Write C code here
    int a,rev =0;
    scanf("%d",&a);
    
    while(a!=0){
        
    int rem = a%10;
        rev = rev*10+rem;
        a= a/10;
    }
    printf("%d",rev);
    
        
    
    return 0;
}

output

123
321

=== Code Execution Successful ===
